{
  "name": "mpn-chat-api",
  "version": "0.0.0",
  "description": "MPN Chat API - Sample with typescript, express and mongoose",
  "author": "Jefferson Follmann <jeff.follmann@gmail.com>",
  "repository": {
    "url": "github.com/jfollmann"
  },
  "main": "build/src/Server.js",
  "types": "build/src/Server.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "nyc": {
    "check-coverage": false,
    "exclude": [
      "**/*.js",
      "build",
      "**/*.test.ts",
      "**/_mocks/*.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "extension": [
      ".ts"
    ],
    "all": true
  },
  "scripts": {
    "start": "node Server.js",
    "start:dev": "nodemon",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "test": "jasmine-ts --config=jasmine.json",
    "test:coverage": "nyc --reporter=lcov npm test"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jasmine": "^3.3.9",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^10.0.3",
    "coveralls": "^3.0.9",
    "gts": "^1.1.2",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^13.3.0",
    "ts-node": "^8.6.2",
    "typescript": "~3.7.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "http-status-codes": "^1.4.0",
    "mongoose": "^5.8.11"
  }
}
